CREATE TABLE artists (
    id INTEGER PRIMARY KEY AUTOINCREMENT, 
    stage_name TEXT,
    real_name TEXT
); 

INSERT INTO artists (stage_name, real_name) VALUES ("Lady Gaga", "Stefani Joanne Angelina Germanotta");
